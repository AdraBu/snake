<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Slider?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.VBox?>

<BorderPane fx:id="borderPane" maxHeight="-Infinity" maxWidth="-Infinity" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/null" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.MainGUI">
    <right>
        <VBox alignment="TOP_CENTER" maxWidth="138.0" minWidth="138.0" spacing="10.0" BorderPane.alignment="CENTER">
            <children>
                <Button mnemonicParsing="false" onAction="#startGame" text="Start" VBox.vgrow="ALWAYS" />
            <CheckBox fx:id="bootCheckBox" mnemonicParsing="false" text="Boot game" />
            <Label fx:id="speedLabel" text="Initial text" />
            <Slider fx:id="speedSlider" blockIncrement="1.0" majorTickUnit="1.0" max="100.0" min="5.0" minorTickCount="0" snapToTicks="true" value="25.0" />
            <Label fx:id="foodLabel" text="Initial text" />
            <Slider fx:id="foodSlider" blockIncrement="1.0" majorTickUnit="1.0" max="15.0" min="1.0" minorTickCount="0" value="1.0" />
            </children>
         <padding>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
         </padding>
        </VBox>
    </right>
</BorderPane>
